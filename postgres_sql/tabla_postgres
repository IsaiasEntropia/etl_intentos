CREATE TABLE cvu_info (
	persona_id serial PRIMARY KEY,
	cvu_num VARCHAR ( 5 ) NOT NULL,
	nombre VARCHAR ( 20 ) NOT NULL);
	

CREATE TABLE apoyo(
   beca_id serial PRIMARY KEY,
   tipo_beca VARCHAR (10) NOT NULL
);

